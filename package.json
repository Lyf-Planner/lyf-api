{
  "name": "lyf-api",
  "version": "v3",
  "description": "Lyf API Server",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev:server": "nodemon --watch './**/*.ts' --exec 'ts-node' src/index.ts",
    "lint": "npx tslint -p tsconfig.json -c tslint.json && npx tsc",
    "lint:fix": "npx tslint -p tsconfig.json -c tslint.json --fix && npx tsc",
    "precommit": "yarn lint:fix",
    "serve": "rm -rf build && npm run build && npm run start",
    "start": "node ./build/src/index.js",
    "test": "jest --silent"
  },
  "author": "Ethan Husband",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^16.11.7",
    "@types/pg": "^8.11.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^8.3.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "nodemon": "^2.0.15",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.4.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-import-group-ordering": "^2.1.2"
  },
  "dependencies": {
    "agenda": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "expo-server-sdk": "^3.7.0",
    "express": "^4.17.1",
    "express-body-parser-error-handler": "^1.0.7",
    "express-rate-limit": "^7.1.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.3",
    "moment-timezone": "^0.5.44",
    "mongodb": "^4.1.4",
    "openweather-api-node": "^3.1.4",
    "p-queue": "6.6.2",
    "pg": "^8.11.5",
    "signature-debouncer": "^1.0.1",
    "typescript": "^4.5.2",
    "uuid": "^8.3.2"
  },
